<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="interceptor.dao.face.InterceptorDao">

	<select id="selectCntByIdPw" resultType="int" parameterType="InterUser">
		SELECT count(*) cnt
		FROM usertb
		<where>
			AND userid = #{userid }
			AND userpw = #{userpw }
		</where>
	</select>
	
	<select id="selectById" resultType="InterUser" parameterType="InterUser">
		SELECT
			userno, userid, usernick
		FROM usertb
		<where>
			AND userid = #{userid }
		</where>
	</select>
	
</mapper>
